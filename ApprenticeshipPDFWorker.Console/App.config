<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog, Version=4.0.0.0, Culture=neutral, PublicKeyToken=5120e14c03d0593c" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <add key ="GovUkBaseUrl" value="https://www.gov.uk"/>
    <add key ="AttachmentDetailsString" value =".attachment-details h2 a"/>
    <add key ="FrameworkKeywordString" value ="Assessment"/>
    <add key ="StandardKeywordString" value="Apprenticeship"/>
  </appSettings>
  <connectionStrings>
    <add name="GovUk" connectionString="Data Source=(localdb)\ProjectsV13;Initial Catalog=ApprenticeshipPDFWorker.Database;Integrated Security=True;Pooling=False;Connect Timeout=30"/>
  </connectionStrings>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd" autoReload="true" throwExceptions="false">
    <!--<extensions>
      <add assembly="SFA.DAS.NLog.Targets.Redis" />
    </extensions>-->
    <variable name="appName" value="das-ci-test" />
    <targets>
      <!--<target xsi:type="Redis" name="RedisLog" appName="${appName}" environmentKey="EnvironmentName" connectionStringKey="LoggingRedisConnectionString" includeAllProperties="true" keySettingsKey="LoggingRedisKey" layout="${message}" />-->
      <target name="c" xsi:type="Console"  layout="[${level:uppercase=true}] ${message}"/>
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" writeTo="c" />
    </rules>
  </nlog>
</configuration>